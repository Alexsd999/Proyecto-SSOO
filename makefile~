# Makefile para ajilizar los pasos

cliente: build/cliente.o build/funciones.o
	gcc -Wall -o cliente cliente.o funciones.o -lsqlite3

cliente.o: src/cliente.c include/cab.h include/funciones.h
	gcc -Wall -c cliente.c

funciones.o: src/funciones.c include/cab.h include/funciones.h
	gcc -Wall -c funciones.c

borrar:
	rm build/*.o bin/*
